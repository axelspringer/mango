{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6.9.2",
          "browsers": ["last 2 versions", "safari >= 7"]
        },
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-vue-jsx",
    "syntax-dynamic-import",
    "external-helpers",
    "transform-regenerator", ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "rollup-regenerator-runtime"
    }]
  ]
}
