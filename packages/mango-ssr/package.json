{
  "name": "@axelspringer/mango-ssr",
  "version": "1.0.0-beta.10",
  "description": "Mango WordPress Server Side Renderer",
  "main": "dist/main.js",
  "bin": {
    "mango-ssr": "./bin/mango-ssr"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir ./dist",
    "build:docker": "docker build --squash -t axelspringer/mango-ssr .",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm run rimraf -- dist node_modules",
    "prebuild": "npm run clean:dist",
    "rimraf": "rimraf",
    "server:dev": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon ./dist/server.js",
    "start": "cross-env NODE_ENV=development npm run build && concurrently \"npm run watch\" \"npm run server:dev\"",
    "watch": "tsc --watch -p tsconfig.json --outDir ./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=6.9.13",
    "npm": ">=3.10.7"
  },
  "author": "Axel Springer SE",
  "contributors": [
    {
      "name": "Sebastian DÃ¶ll",
      "email": "sebastian.doell@axelspringer.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@axelspringer/mango-plugin-rss": "^1.0.0-beta.10",
    "@types/express": "^4.11.0",
    "@types/node": "^10.3.0",
    "@types/winston": "^2.3.7",
    "rimraf": "^2.6.2",
    "vue": "^2.5.16"
  },
  "dependencies": {
    "@axelspringer/mango-config": "^1.0.0-beta.10",
    "assert": "^1.4.1",
    "chalk": "^2.4.0",
    "http-graceful-shutdown": "^2.1.1",
    "http-status-codes": "^1.3.0",
    "koa": "^2.5.1",
    "koa-body": "^2.6.0",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-mount": "^3.0.0",
    "koa-pino-logger": "^2.1.3",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "koa-static": "^4.0.3",
    "koa-static-server": "^1.3.4",
    "koa-webpack": "^4.0.0",
    "lodash": "^4.17.10",
    "memory-fs": "^0.4.1",
    "ts-node": "^6.1.0",
    "tslib": "^1.8.0",
    "typescript": "^2.6.2",
    "upath": "^1.1.0",
    "vue-server-renderer": "^2.5.13",
    "webpack": "^4.10.2",
    "yargs": "^11.0.0"
  }
}
