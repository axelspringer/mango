{
  "name": "@axelspringer/mango-plugin-customizer",
  "version": "0.8.1",
  "description": "Mango API plugign for customizer extensitons to WP REST",
  "main": "dist/customizer.common.js",
  "web": "dist/customizer.js",
  "module": "dist/customizer.esm.js",
  "scripts": {
    "build": "rollup -c scripts/config.js",
    "build:types": "npm run tsc -- -p tsconfig.json --declaration --declarationDir ./types",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm run rimraf -- dist node_modules",
    "prebuild": "npm run clean:dist",
    "rimraf": "rimraf",
    "start": "npm run watch",
    "tsc": "tsc",
    "watch": "rollup -w -c scripts/config.js"
  },
  "files": [
    "data/*.json",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=6.9.13",
    "npm": ">=3.10.7"
  },
  "author": "Axel Springer SE",
  "contributors": [
    {
      "name": "Steve Junker",
      "email": "steve.junker@axelspringer.de"
    }
  ],
  "peerDependencies": {
    "@axelspringer/mango-api": "^0.8.1",
    "graphql": "^0.12.3",
    "graphql-iso-date": "^3.3.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@axelspringer/mango-api": "^0.8.1",
    "@types/node": "^9.4.7",
    "babel-core": "^6.26.0",
    "lodash": "^4.17.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.12.0",
    "rollup-plugin-vue": "^3.0.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "tslib": "^1.8.0"
  }
}
