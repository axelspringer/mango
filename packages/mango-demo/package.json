{
  "name": "@axelspringer/mango-demo",
  "version": "0.1.10",
  "description": "Mango Demo Plugin",
  "private": true,
  "keywords": [
    "vue",
    "webpack",
    "typescript"
  ],
  "author": {
    "name": "Axel Springer SE"
  },
  "contributors": [{
    "name": "Sebastian DÃ¶ll",
    "email": "sebastian.doell@axelspringer.com"
  }],
  "bundlesize": [{
      "path": "./public/client/vendor.*.js",
      "maxSize": "400 kB"
    },
    {
      "path": "./public/client/polyfills.*.js",
      "maxSize": "120 kB"
    }
  ],
  "homepage": "https://github.com/axelspringer/mango",
  "license": "UNLICENSED",
  "scripts": {
    "build:docker": "npm run build && docker build --squash -t mango:latest .",
    "build:server": "cross-env NODE_ENV=production webpack --progress --profile --bail --debug",
    "build:client": "cross-env NODE_ENV=production webpack --progress --profile --bail --debug",
    "build": "npm run build:client && npm run build:server",
    "ci": "npm test --runInBand && bundlesize",
    "clean:public": "npm run rimraf -- public",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm run clean:public && start",
    "clean": "npm cache clean --force && npm run rimraf -- doc coverage public node_modules",
    "cross-env": "cross-env",
    "dev": "cross-env NODE_ENV=development",
    "docker": "docker",
    "mock": "cross-env NODE_ENV=development ./node_modules/.bin/mango-api -m --plugin pagemanager",
    "help": "node node_modules/npm-scripts-help",
    "jest": "jest",
    "rimraf": "rimraf",
    "prebuild": "npm run clean:public",
    "preversion": "npm test",
    "prestart": "npm run clean:public",
    "prod": "cross-env NODE_ENV=production",
    "server": "cross-env NODE_ENV=production ./node_modules/.bin/mango-ssr --serve public --bundle public/vue-ssr-server-bundle.json -m public/vue-ssr-client-manifest.json -t public/index.html -w config/configs",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/mango-ssr --serve public --bundle public/vue-ssr-server-bundle.json -m public/vue-ssr-client-manifest.json -t public/index.html -w config/configs",
    "test": "cross-env NODE_ENV=testing npm run jest",
    "test:bundlesize": "npm run bundlesize",
    "watch:test": "jest --watch",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@axelspringer/mango-api": "^0.1.10",
    "@axelspringer/mango-plugin-navigation": "^0.1.8",
    "@axelspringer/mango-plugin-pagemanager": "^0.1.8",
    "@axelspringer/mango-ssr": "^0.1.10",
    "@axelspringer/mango-vue": "^0.1.10",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link-http": "^1.3.1",
    "core-js": "^2.5.1",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.6.0",
    "isomorphic-fetch": "^2.2.1",
    "lazysizes": "^4.0.1",
    "nav-scroll-spy": "^1.0.2",
    "scroll-window-to-element": "^1.0.5",
    "shufflejs": "^5.0.3",
    "tiny-slider": "^2.3.11",
    "vue": "^2.5.8",
    "vue-apollo": "^3.0.0-alpha.2",
    "vue-class-component": "^6.1.0",
    "vue-i18n": "^7.3.2",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.8",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.11.7",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.6.3",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.49",
    "@types/uglify-js": "^2.6.29",
    "autodll-webpack-plugin": "^0.3.3",
    "autoprefixer": "^7.1.6",
    "avoriaz": "^6.0.1",
    "awesome-typescript-loader": "^3.3.0",
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "brotli-webpack-plugin": "^0.5.0",
    "bundlesize": "^0.15.3",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.8",
    "cssnano": "^3.10.0",
    "csx": "^8.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-critical-webpack-plugin": "^1.0.1",
    "html-elements-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "jest": "^21.2.1",
    "jest-vue": "^0.8.2",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "npm-scripts-help": "^0.7.0",
    "offline-plugin": "^4.8.4",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "^2.0.8",
    "preload-webpack-plugin": "^2.0.0",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.2.1",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "tslib": "^1.8.1",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-loader": "^3.5.1",
    "typescript": "2.6.2",
    "typestyle": "^1.5.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "vue-loader": "^13.4.0",
    "vue-property-decorator": "^6.0.0",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.es2015.json",
        "useBabelrc": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axelspringer/mango.git"
  },
  "bugs": {
    "url": "https://github.com/axelspringer/mango/issues"
  },
  "engines": {
    "node": ">= 6.9.1",
    "npm": ">= 3"
  }
}
